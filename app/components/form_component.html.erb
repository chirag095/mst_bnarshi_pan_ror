<%= form_with url: franchies_contact_path(), method: :post, id: "franchiesformcontact" do %>
          <%= text_field_tag :full_name, nil, placeholder: "Enter Name", class: "inputcol", required: true %>
          <%= email_field_tag :email, nil, placeholder: "Enter Email", class: "inputcol", required: true %>
          <%= number_field_tag :contact, nil, placeholder: "Enter Phone", class: "inputcol", required: true %>          
         <%= select_tag :enquiry_form, options_for_select(User::OUTLATE_TITLE), prompt: "Select Model", class: "inputcol", required: true %>

          <%= select_tag :state, options_from_collection_for_select(@states, :id, :title), { prompt: "Select State", class: "inputcol", required: true } %>
          <%= text_area_tag :description, nil, placeholder: "Write something..", class: "inputcol", style: "height:10vw;", required: true %>
          <%= submit_tag "Submit", class: "inputsubmitcol", onclick: "sendToWhatsapp()", style: "margin-top: 15px; font-size: medium; " %>
        <% end %>