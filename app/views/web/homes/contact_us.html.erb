body {
    font-family: Arial, sans-serif;
    background: #121212;
}

/* Navbar */
.navbar {
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 15px 30px;
    background: #292929;
    position: fixed;
    width: 100%;
    top: 0;
    z-index: 1000;
}

/* Menu containers */
.menu-container {
    display: flex;
    justify-content: space-between;
    flex: 1;
}

.menu-left,
.menu-right {
    display: flex;
}

.nav-menu {
    list-style: none;
    display: flex;
    gap: 20px;
}

.nav-menu a {
    text-decoration: none;
    color: #fff;
    font-size: 18px;
    padding: 10px 15px;
    transition: transform 0.3s, box-shadow 0.3s;
}

.nav-menu a:hover {
    transform: translateY(-5px);
    box-shadow: 0 5px 10px rgba(255, 255, 255, 0.2);
}

/* Centering the logo */
.logo {
    position: absolute;
    left: 50%;
    transform: translateX(-50%);
}

.logo img {
    height: 50px;
}

/* Contact section (aligned right) */
.contact {
    margin-left: auto;
    position: relative;
}

/* 3D Effect for Phone Number */
.phone-3d {
    color: #fff;
    font-size: 20px;
    text-decoration: none;
    padding: 10px 15px;
    background: linear-gradient(145deg, #1e1e1e, #2e2e2e);
    border-radius: 8px;
    box-shadow: 3px 3px 10px rgba(0, 0, 0, 0.5), 
                -3px -3px 10px rgba(255, 255, 255, 0.1);
    transition: transform 0.3s, box-shadow 0.3s;
}

.phone-3d:hover {
    transform: translateY(-3px);
    box-shadow: 5px 5px 15px rgba(0, 0, 0, 0.5),
                -5px -5px 15px rgba(255, 255, 255, 0.2);
}

/* Hamburger Menu */
.hamburger {
    display: none;
    font-size: 28px;
    color: #fff;
    cursor: pointer;
}

/* Mobile Responsive */
@media (max-width: 768px) {
    .menu-container {
        display: none;
        flex-direction: column;
        background: #292929;
        position: absolute;
        top: 60px;
        left: 0;
        width: 100%;
        padding: 10px;
    }

    .menu-container.active {
        display: flex;
        flex-direction: column;
        align-items: center;
    }

    .menu-left, .menu-right {
        flex-direction: column;
        align-items: center;
        width: 100%;
    }

    .logo {
        position: relative;
        left: auto;
        transform: none;
        text-align: center;
        flex: 1;
    }

    .contact {
        display: block;
        text-align: center;
        padding-right: 15px;
        margin-top: 10px;
    }

    .hamburger {
        display: block;
    }
}

   /* Breadcrumb Area */
.breadcrumb-area {
    padding: 100px 0;
    background-size: cover;
    background-position: center;
    position: relative;
}
.breadcrumb-area h1 {
    font-size: 36px;
    font-weight: 700;
    text-transform: uppercase;
    margin-bottom: 15px;
}
.breadcrumb {
    list-style: none;
    padding: 0;
    display: inline-flex;
    justify-content: center;
}
.breadcrumb li {
    display: inline;
    margin: 0 5px;
}
.breadcrumb li a {
    color: #fff;
    text-decoration: none;
}
.breadcrumb li.active {
    color: #ffcc00;
}

/* Contact Form */
.contact-us-area {
    padding: 80px 0;
    background: #f9f9f9;
}
.contact-box {
    display: flex;
    flex-wrap: wrap;
    gap: 30px;
}
.form-box {
    background: #fff;
    padding: 30px;
    border-radius: 10px;
    box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);
}
.form-content h3 {
    font-size: 24px;
    margin-bottom: 20px;
}
.form-control {
    width: 100%;
    padding: 10px;
    margin-bottom: 15px;
    border: 1px solid #ddd;
    border-radius: 5px;
}
button#submit {
    background: #1298d5;
    color: white;
    padding: 12px;
    border: none;
    cursor: pointer;
    border-radius: 5px;
    font-size: 16px;
    display: inline-block;
}
button#submit:hover {
    background: #0f7abf;
}

/* Contact Info */
.info {
    padding: 95px;
    background: #fff;
    border-radius: 10px;
    box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);
}
.info h2 {
    font-size: 28px;
    margin-bottom: 15px;
}
.address-items .item {
    display: flex;
    align-items: center;
    margin-bottom: 15px;
}
.address-items .icon {
    font-size: 20px;
    margin-right: 10px;
    color: #1298d5;
}
 .contact-info {
        font-size: 18px;
        font-weight: bold;
        font-family: Arial, sans-serif;
        line-height: 1.8;
    }
    .contact-info i {
        color: #007bff;
        margin-right: 10px;
    }
