<script>
    const postsPerPage = 6; // Number of blogs per page
    let currentPage = 1; // Default to first page
    const posts = document.querySelectorAll(".blog-post");
    const totalPages = Math.ceil(posts.length / postsPerPage);

    function showPage(page) {
        let start = (page - 1) * postsPerPage;
        let end = start + postsPerPage;
        
        posts.forEach((post, index) => {
            post.style.display = (index >= start && index < end) ? "block" : "none";
        });

        updatePagination(page);
    }

    function updatePagination(page) {
        let paginationNumbers = document.getElementById("pagination-numbers");
        paginationNumbers.innerHTML = "";

        for (let i = 1; i <= totalPages; i++) {
            let btn = document.createElement("button");
            btn.textContent = i;
            btn.onclick = function () {
                currentPage = i;
                showPage(currentPage);
            };
            if (i === page) {
                btn.style.backgroundColor = "#cd5906"; // Highlight current page
            }
            paginationNumbers.appendChild(btn);
        }
    }

    function changePage(direction) {
        let newPage = currentPage + direction;
        if (newPage > 0 && newPage <= totalPages) {
            currentPage = newPage;
            showPage(currentPage);
        }
    }

    showPage(currentPage); // Initialize
</script>

